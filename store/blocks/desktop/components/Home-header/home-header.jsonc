      "width": 50,
      "mobileWidth": 30,
      "mobileHeight": 30
    }
  },
  "header-row#2-desktop": {
    "children": [
      "header-border",
      "rich-text#header",
      "header-spacer",
      //"search-bar",
      "header-spacer",
      "login",
      "minicart.v2"
    ],
    "props": {
      "fullWidth": "true",
      "blockClass": "header-layout--containert"
    }
  },
  "rich-text#header": {
    "props": {
      "blockClass": "section__one--title",
      "text": "ENV√çO GRATIS A PARTIR DE $400 MIL",
      "textColor": "#f5f5f5"
    }
  },
  "menu#header-menu--desktop": {
    "children": [
      "menu-item#department__one",
      "menu-item#department__two",
      "menu-item#department__three",
      "menu-item#department__four",
      "menu-item#department__five",
      "menu-item#department__six"
    ]
  },
  "menu-item#department__one": {
    "props": {
      "id": "category-ne",
      "type": "custom",
      "itemProps": {
        "type": "external",
        "href": "https://www.google.com/",
        "noFollow": true,
        "tagTitle": "google title",
        "text": "Smartphones"
      }
    },
    "blocks": ["submenu.accordion#category__one"]
  },
  "submenu.accordion#category__one": {
    "children": ["menu#department__one"]
  },
  "menu#department__one": {
    "children": ["slider-layout#home__slider--children"]
  },
