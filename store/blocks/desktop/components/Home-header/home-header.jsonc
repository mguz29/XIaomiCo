{
  "header-layout.desktop": {
    "title": "header Desktop",
    "children": ["header-row#2-desktop", "header-row#1-desktop"],
    "props": {
      "blockClass": "header-layou--containert#home"
    }
  },
  "header-row#1-desktop": {
    "props": {
      "blockClass": "row1--logo",
      "verticalAlign": "middle"
