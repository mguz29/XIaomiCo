  },
  "menu-item#department__oneDesktop": {
    "props": {
      "id": "category-ne",
      "type": "custom",
      "blockClass": "header--menu__item",
      "itemProps": {
        "type": "external",
        "href": "https://www.google.com/",
        "noFollow": true,
        "tagTitle": "google title",
        "text": "Smartphones"
      }
    },
    "blocks": ["submenu#category__oneDesktop"]
  },
